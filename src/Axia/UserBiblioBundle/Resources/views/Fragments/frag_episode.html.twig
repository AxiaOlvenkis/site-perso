<p class="{{ biblio.id }}">
    {% if is_granted("ROLE_ADMIN") %}
        <button class="moinsPlus" id="{{ biblio.id }}">
            <span class="glyphicon glyphicon-minus" id="moins" aria-hidden="true"></span>
        </button>
        <span class="number">{{ biblio.dernierVu }} </span> / {{ biblio|has_a_voir }}
        <button class="moinsPlus" id="{{ biblio.id }}">
            <span class="glyphicon glyphicon-plus" id="plus" aria-hidden="true"></span>
        </button>
    {% else %}
        <span class="number">{{ biblio.dernierVu }} </span> / {{ element.nbEpisode }}
    {% endif %}
</p>
